<%= form_for(@category) do |f| %>

  <div class="name">
        <%= f.label :name %>
        <%= f.text_field :name %>
  </div>
  <div class="thumburl">
        <%= f.label :thumburl %>
        <%= f.text_field :thumburl %>
  </div>
  <div class="actions">
        <%= f.submit %>
  </div>
<% end %>
